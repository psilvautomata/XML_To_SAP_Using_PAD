Outlook.Launch Instance=> OutlookInstance
ON ERROR
    REPEAT ACTION
END
CALL 'Seleção de Usina'
IF SelectedItem = $'''SteelPlant1''' THEN
    CALL SteelPlant1
END
IF SelectedItem = $'''SteelPlant2''' THEN
    CALL SteelPlant2
END
IF SelectedItem = $'''SteelPlant3''' THEN
    CALL SteelPlant3
END
IF SelectedItem = $'''All''' THEN
    CALL All
END
